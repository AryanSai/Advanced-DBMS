# Print the tree
    def print_tree(self):
        lst = [self.root]
        levels = [0]

        while len(lst) != 0:
            x = lst.pop(0)
            lev = levels.pop(0)
            if not x.check_leaf:
                for i, item in enumerate(x.pointers):
                    print("internal: ",item.values)
                    lst.append(item)
                    levels.append(lev + 1)
            else:
                for i, item in enumerate(x.pointers):
                    print("leaf: ",item.values)
                    pass
                print(x.file_offsets, x.values)